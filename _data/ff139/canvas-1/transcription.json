      "body": {
        "type": "TextualBody",
        "value": "<font color=\"navy\">Marilyn Nelson. M-a-r-i-l-y-n  N-e-l-s-o-n.</font>",
        "format": "text/plain"
      },
      "target": {
        "source": "https://JMU-AudiAnnotate.github.io/FFPC-Video-Transcriptions/ff139/canvas-1/canvas",
